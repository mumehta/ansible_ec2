<VirtualHost *:80>

    ServerName munish.cf

    #RewriteEngine On
    #RewriteCond %{HTTPS} off
    #RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}

</VirtualHost>

<VirtualHost *:443>
    ServerName munish.cf
    SSLEngine on

    SSLCertificateFile "/etc/letsencrypt/certs/fullchain_munish.cf"
    SSLCertificateKeyFile "/etc/letsencrypt/keys/munish.cf.key"

    SSLProtocol TLSv1.1 TLSv1.2
    SSLCipherSuite  HIGH:!aNULL:!MD5


</VirtualHost>